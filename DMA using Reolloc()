include <stdio.h>
#include <stdlib.h>

int main() {
    int *p;
    p = (int*) malloc(sizeof(int));    

    *p = 10;                           
    p = (int*) realloc(p, 2 * sizeof(int)); 
    p[1] = 20;                         

    printf("%d %d\n", p[0], p[1]);      
    free(p);                           
    return 0;
}
